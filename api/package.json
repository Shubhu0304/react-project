{
  "name": "airbnb-private-api",
  "version": "1.0.7",
  "start": "./index.ts", 
  "types": "./lib/index.d.ts",
  "homepage": "https://github.com/drawrowfly/airbnb-private-api",
  "bugs": {
    "url": "https://github.com/drawrowfly/airbnb-private-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drawrowfly/airbnb-private-api.git"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "index.ts",
    "build": "rimraf lib && tsc && prettier --config ./.prettierrc.js --write './lib/**/*.js'",
    "format": "prettier --config ./.prettierrc.js --write './src/**/*.ts'",
    "lint": "eslint ./src/**/*.ts",
    "test": "jest --coverage --verbose"
  
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.7.2",
    "lodash": "^4.17.15",
    "node": "^19.6.1",
    "pretty-error": "^2.1.2",
    "react": "^18.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "uuid": "^3.4.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.45",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  },
  "description": ""
}
